{
  "name": "sup-ftween-plugin",
  "version": "0.3.0",
  "description": "Easy tweening (animation of values) for the supGame system for Superpowers, the extensible HTML5 2D+3D game engine.",
  "homepage": "https://github.com/florentpoujol/superpowers-ftween-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "keywords": [
    "superpowers",
    "plugin",
    "tween",
    "interpolation",
    "easing",
    "animation"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin/issues"
  },
  "scripts": {
    "compilets": "tsc api/fTween.ts --out gitignore/fTween.js --module commonjs --target ES5",
    "gendoc": "typedoc --out public/docs/ api/fTween.ts --mode file --target ES5 --name \"fTween plugin\" --readme tutorial.md --verbose",

    "linkplugindev": "mklink /j \"../../app/systems/supGame/plugins/florentpoujol/fTween\" \"./\"",
    "linkprojectdev": "mklink /j \"../../data/projects/fTween\" \"./project/fTween\"",

    "linkpluginrelease": "mklink /j \"../../../release/app/systems/supGame/plugins/florentpoujol/fTween\" \"./\"",
    "linkprojectrelease": "mklink /j \"../../../release/data/projects/fTween\" \"./project/fTween\""
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "license": "MIT"
}
