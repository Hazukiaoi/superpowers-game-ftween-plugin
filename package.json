{
  "name": "superpowers-game-ftween-plugin",
  "version": "0.4.0",
  "description": "Easy tweening (animation of values) for the supGame system for Superpowers, the extensible HTML5 2D+3D game engine.",
  "homepage": "https://github.com/florentpoujol/superpowers-game-ftween-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-game-ftween-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "keywords": [
    "superpowers",
    "plugin",
    "tween",
    "interpolation",
    "easing",
    "animation"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-game-ftween-plugin/issues"
  },
  "scripts": {
    "gendoc": "typedoc --out gitignore/docs/ typescriptAPI/fTween.ts --mode file --target ES5 --name \"fTween plugin\" --readme tutorial.md",
