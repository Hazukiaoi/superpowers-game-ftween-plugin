{
  "name": "sup-ftween-plugin",
  "version": "0.1.2",
  "description": "Easy tweening (animation of values) for Superpowers, the extensible HTML5 2D+3D game engine.",
  "homepage": "https://github.com/florentpoujol/superpowers-ftween-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "dependencies": {},
  "devDependencies": {
    "typedoc": "0.2.x",
    "typescript": "1.4.x",
    "mocha": "2.2.x"
  },
  "keywords": [
    "superpowers",
    "plugin",
    "tween",
    "interpolation",
    "easing",
    "animation"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin/issues"
  },
  "scripts": {
    "compilets": "tsc api/fTween.ts --out test/fTween.js --module commonjs --target ES5",
    "gendoc": "typedoc --out public/docs/ api/fTween.ts --mode file --target ES5 --name \"fTween plugin\" --readme tutorial.md --verbose --includeDeclarations",

    "test": "mocha test/index.js",
    
    "linksrcdev": "mklink /j \"../../app/plugins/florentpoujol/fTween\" \"./\"",
    "linksrcrelease": "mklink /j \"../../../release/app/plugins/florentpoujol/fTween\" \"./\"",

    "linkprojdev": "mklink /j \"../../app/projects/fTween\" \"./project/fTween\"",
    "linkprojrelease": "mklink /j \"../../../localprojects/fTween\" \"./project/fTween\""
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "directories": {
    "doc": "docs"
  },
  "license": "MIT"
}
