{
  "name": "sup-ftween-plugin",
  "version": "0.1.1",
  "description": "Easy tweening (animation of values) for Superpowers, the extensible HTML5 2D+3D game engine.",
  "homepage": "https://github.com/florentpoujol/superpowers-ftween-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "dependencies": {},
  "devDependencies": {
    "typedoc": "0.2.x",
    "typescript": "1.4.x"
  },
  "keywords": [
    "superpowers",
    "plugin",
    "tween",
    "interpolation",
    "easing",
    "animation"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-ftween-plugin/issues"
  },
  "scripts": {
    "gendoc": "typedoc --out docs/ api/fTween.ts --mode file --target ES5 --name \"fTween plugin\" --readme tutorial.md --verbose --includeDeclarations",
    "compilets": "tsc api/fTween.ts --out gitignore/fTween.js --module commonjs --target ES5",
    "mklink": "mklink /j \"../../superpowers/plugins/florentpoujol/fTween\" \"./\""
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "directories": {
    "doc": "docs"
  },
  "license": "MIT"
}
